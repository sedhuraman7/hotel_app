{
    "framework": "nextjs",
    "buildCommand": "npx prisma generate && next build",
    "installCommand": "npm install",
    "headers": [
        {
            "source": "/api/rfid/check",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=0"
                }
            ]
        }
    ]
}